<module rename-to='webapp'>
    <inherits name='playn.PlayN'/>
    <inherits name='Assets'/>
    <inherits name='com.google.common.collect.Collect'/>
    <inherits name='com.google.common.cache.Cache'/>
    <inherits name='com.google.common.io.Io'/>
    <!-- <inherits name="bolts.Bolts"/> -->

    <source path='data'/>
    <source path='flare'/>
    <source path='toolkit'/>

    <entry-point class='org.nting.flare.app.web.WebApp'/>

    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>

    <!--<set-property name="user.agent" value="safari,gecko1_8,ie9"/>-->
</module>
